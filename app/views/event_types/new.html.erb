<%= content_for :head do %>
  <%= stylesheet_link_tag 'pickr/classic.min' %>
  <%= javascript_include_tag 'pickr/pickr.es5.min' %>
  <%= javascript_include_tag 'jquery-validate/jquery.validate' %>
  <%= javascript_include_tag 'jquery-validate/localization/messages_es' %>
  <%= javascript_include_tag 'app/event_types/new' %>
<% end %>
<div class="row">
  <div class="col-md-2 col-sm-1 col-xs-1"></div>
  <div class="col-md-8 col-sm-10 col-xs-10">
    <div class="card">
      <div class="card-body">
        <%= render partial: 'form', locals: { event_type: @event_type } %>
      </div>
    </div>
  </div>
  <div class="col-md-2 col-sm-1 col-xs-1"></div>
</div>